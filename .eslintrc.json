{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
        "standard"
    ],
    "parserOptions": {
        "ecmaVersion": 12,
        "sourceType": "module"
    },
    "rules": {
        "semi": [2, "always"],
        "no-trailing-spaces": [2, { "skipBlankLines": true }],
        "comma-dangle":["error", "always-multiline"]
    }
}
